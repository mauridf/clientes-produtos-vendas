  <DataSets>
    <DataSet Name="DataSetVendas">
      <Query>
        <DataSourceName>DataSetVendas</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dataInicio">
            <Value>=Parameters!DataInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dataFim">
            <Value>=Parameters!DataFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          
            SELECT v.vendaid AS VendaId, 
                   p.nome AS ProdutoNome, 
                   iv.quantidade AS Quantidade, 
                   p.preco AS Preco, 
                   v.datavenda AS DataVenda
            FROM Vendas v
            JOIN ItemVenda iv ON v.vendaid = iv.vendaid
            JOIN Produto p ON iv.produtoid = p.produtoid
            WHERE v.datavenda BETWEEN @dataInicio AND @dataFim
          
        </CommandText>
      </Query>
      <Fields>
        <Field Name="VendaId">
          <DataField>VendaId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProdutoNome">
          <DataField>ProdutoNome</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Quantidade">
          <DataField>Quantidade</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Preco">
          <DataField>Preco</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DataVenda">
          <DataField>DataVenda</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
